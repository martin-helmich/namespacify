{
    "name": "helmich/namespacify",
    "description": "Convert PHP 5.2-style classes to namespaces.",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/martin-helmich",
    "authors": [
        {
            "name": "Martin Helmich",
            "email": "m.helmich@mittwald.de",
            "role": "lead"
        }
    ],
    "support": {
        "issues": "https://github.com/martin-helmich/namespacify/issues"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.4",
        "symfony/console": "2.4.*",
        "symfony/dependency-injection": "2.4.*",
        "nikic/php-parser": "~1.4"
    },
    "require-dev": {
        "behat/behat": "~3.0"
    },
    "bin": [
        "namespacify"
    ],
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": {
            "Helmich\\Namespacify": "src/"
        }
    }
}
